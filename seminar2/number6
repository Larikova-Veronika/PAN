#include <vector>
#include <iostream>
#include <iomanip>
#include <algorithm>
using namespace std;

int main() {
	std::vector<double> heights;
	    const double v0 = 50.0, g = 9.8, dt = 1.0;
	    double h=0.0;
	    for (double t = 0.0; t <= 1000; t += dt) {
	    	h = v0 * t - 0.5 * g * t * t;
	        if (h < 0) break;
	        heights.push_back(h);
	    }
	auto new_end = std::remove_if(heights.begin(), heights.end(), [](double h){return h < 10.0; });
	heights.erase(new_end, heights.end());
	return 0;
}
