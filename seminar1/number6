#include <iostream>
#include <iomanip>
using namespace std;

int main() {
	const double S=20;
	const double CL=0.033;
	double V[5]={0,10,15,18,19.5};
	double ro[5]={1.39,1.34,1.26,1.20,1.14};
	cout << "\nТаблица траектории движения:" << endl;
	cout << " t (с)  |   V (м)   |   ro (кг/м^3)  |   L (H)   " << endl;
	cout << "-----------------------------" << endl;
	for (int i=0;i<5;i++){
		double L = S*V[i]*V[i]*2*ro[i]*CL/2;
		 cout << i+1 << " | " << V[i] << " | " << ro[i] << " | " << L << endl;

	}

	return 0;
}
